{
    "version": 2,
    "rewrites": [
      { "source": "/routes/protected/(.*)", "destination": "/app.js" },
      { "source": "/routes/private/(.*)", "destination": "/app.js" },
      { "source": "/(.*)", "destination": "/app.js" }
    ],
    "builds": [
      { "src": "app.js", "use": "@vercel/node" }
    ]
  }